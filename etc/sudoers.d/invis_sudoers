Cmnd_Alias      CREATEHOME = /usr/bin/createhome
Cmnd_Alias      DELETEHOME = /usr/bin/deletehome
Cmnd_Alias      CREATEGROUPSHARE = /usr/bin/creategroupshare
Cmnd_Alias      FETCHPHP = /var/lib/cornaz/bin/fetchcopy
Cmnd_Alias      HOLIDAY = /var/lib/cornaz/bin/holiday
Cmnd_Alias      BACKHOME = /var/lib/cornaz/bin/backhome

wwwrun  ALL = (root) NOPASSWD:CREATEHOME
wwwrun  ALL = (root) NOPASSWD:DELETEHOME
wwwrun  ALL = (root) NOPASSWD:CREATEGROUPSHARE
wwwrun  ALL = (root) NOPASSWD:FETCHPHP
wwwrun  ALL = (root) NOPASSWD:HOLIDAY
wwwrun  ALL = (root) NOPASSWD:BACKHOME

